{
  "name": "jonesiscoding/pleasing",
  "description": "Caching engine for Assetic.  Assists with minification & compiling of LESS, SASS, and JS assets.",
  "keywords": [
      "sass",
      "scss",
      "php",
      "assetic",
      "pleasing",
      "sassc",
      "minify",
      "js",
      "css"
  ],
  "homepage": "https://www.github.com/jonesiscoding/pleasing",
  "version": "2.2",
  "authors": [
      {
          "name": "Aaron M Jones",
          "homepage": "http://www.jonesiscoding.com"
      }
  ],
  "license": "MIT",
  "autoload": {
      "psr-4": {
          "XQ\\Pleasing\\": "src"
      }
  },
  "replace": {
    "exactquery/pleasing": "^1.0"
  },
  "require": {
      "kriswallsmith/assetic": "~1.3",
      "leafo/lessphp": "~0.5.0",
      "tedivm/jshrink": "~1.1",
      "jonesiscoding/pleasing-filters": "^2.0"
  },
  "suggest": {
    "leafo/scssphp": "Used as a backup to SASSC to compile SCSS.  Will function more slowly than sassc binary.",
    "symfony/http-foundation": "For header responses with better error checking."
  },
  "require-dev": {
    "symfony/yaml": "^3.3",
    "symfony/http-foundation": "^3.3"
  }
}
